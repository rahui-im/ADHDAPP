    "@supabase/supabase-js": "^2.56.1",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/react-router-dom": "^5.3.3",
    "canvas-confetti": "^1.9.3",
    "framer-motion": "^11.18.2",
    "i18next": "^25.4.2",
